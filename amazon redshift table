CREATE TABLE weather_cities (
	lon decimal(10,2),
	lat decimal(10,2),
    main char(20),
    description char(40),
    temp decimal(10,2),
    feels_like decimal(10,2),
    temp_min decimal(10,2),
    temp_max decimal(10,2),
    pressure decimal(10,2),
    humidity decimal(10,2),
    visibility integer,
    speed decimal(10,2),
    dt timestamp,
    country char(4),
    name char(40),
    PRIMARY KEY (name, dt)
);

select * from weather_cities